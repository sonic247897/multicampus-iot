package basic;

import java.io.OutputStream;

// 키보드로 입력하는 값을 아두이노로 내보내기 위한 스레드 - Serial 통신
public class SerialArduinoWriterThread extends Thread{
	OutputStream out;
	
	public SerialArduinoWriterThread(OutputStream out) {
		super();
		this.out = out;
	}

	@Override
	public void run() {
		int c = 0;
		while(int c = System.in.read())){
			
		}
	}
	
}
